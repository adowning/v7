{
  "name": "andrews-app",
  "version": "0.1.3",
  "private": true,
  "description": "An app for Andrews",
  "author": {
    "name": "adowning",
    "email": "ashdowning@gmail.com"
  },
  "scripts": {
    "serve": "exec npm run electron:serve",
    "build": "vue-cli-service build",
    "electron:build": "vue-cli-service electron:build",
    "electron:serve": "fuser -k 8080/tcp & vue-cli-service electron:serve",
    "postinstall": "electron-builder install-app-deps",
    "postuninstall": "electron-builder install-app-deps",
    "electron:generate-icons": "electron-icon-builder --input=./public/icon.png --output=build --flatten",
    "build:dir": "node config/build.js && electron-builder --dir"
  },
  "main": "background.js",
  "dependencies": {
    "core-js": "^2.6.5",
    "electron-icon-builder": "^1.0.1",
    "electron-is": "^3.0.0",
    "electron-log": "^3.0.8",
    "electron-store": "^5.0.0",
    "electron-updater": "^4.1.2",
    "log4js": "^5.1.0",
    "moment": "^2.24.0",
    "vue": "^2.6.6",
    "vue-class-component": "^6.0.0",
    "vue-moment": "^4.0.0",
    "vue-parse": "^0.1.0",
    "vue-property-decorator": "^8.1.1",
    "vue-router": "^3.1.3",
    "vuetify": "^2.0.17"
  },
  "devDependencies": {
    "@types/parse": "^2.2.2",
    "@types/toastr": "^2.1.37",
    "@vue/cli-plugin-babel": "^3.5.0",
    "@vue/cli-plugin-typescript": "^3.5.0",
    "@vue/cli-service": "^3.5.0",
    "electron": "^6.0.0",
    "electron-simple-publisher": "^0.6.2",
    "sass": "^1.17.4",
    "sass-loader": "^7.1.0",
    "toastr": "^2.1.4",
    "typescript": "^3.2.1",
    "vue-cli-plugin-electron-builder": "^1.4.0",
    "vue-cli-plugin-vuetify": "^0.6.3",
    "vue-template-compiler": "^2.5.21",
    "vuetify-loader": "^1.2.2"
  },
  "postcss": {
    "plugins": {
      "autoprefixer": {}
    }
  },
  "browserslist": [
    "> 1%",
    "last 2 versions",
    "not ie <= 8"
  ]
}
